<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Emacs" modifieddefault="false">
   <Context id="Global">
      <Action id="delete-previous">
         <Stroke code="VK_BACK_SPACE"/>
         <Stroke code="VK_BACK_SPACE" shift="on"/>
         <Stroke code="VK_H" ctrl="on"/>
      </Action>
      <Action id="selection-backward">
         <Stroke code="VK_LEFT" shift="on"/>
         <Stroke alt="on" code="VK_B" shift="on"/>
         <Stroke code="VK_KP_LEFT" shift="on"/>
         <Stroke code="VK_B" meta="on" shift="on"/>
      </Action>
      <Action id="undo">
         <Stroke code="VK_SLASH" ctrl="on"/>
      </Action>
      <Action id="expand-all-folds">
         <Stroke code="VK_EQUALS" ctrl="on" shift="on"/>
         <Stroke code="VK_MINUS" ctrl="on"/>
      </Action>
      <Action id="selection-next-word">
         <Stroke code="VK_RIGHT" ctrl="on" shift="on"/>
         <Stroke alt="on" code="VK_F" ctrl="on"/>
         <Stroke code="VK_KP_RIGHT" ctrl="on" shift="on"/>
         <Stroke code="VK_F" ctrl="on" meta="on"/>
      </Action>
      <Action id="save-workspace"/>
      <Action id="find-files"/>
      <Action id="eval-cell-and-advance">
         <Stroke code="VK_ENTER" ctrl="on" shift="on"/>
         <Sequence>
            <Stroke code="VK_U" ctrl="on"/>
            <Stroke code="VK_F" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="insert-tab">
         <Stroke code="VK_TAB"/>
         <Stroke code="VK_I" ctrl="on"/>
      </Action>
      <Action id="scroll-up">
         <Stroke code="VK_DOWN" ctrl="on"/>
         <Stroke alt="on" code="VK_N"/>
         <Stroke code="VK_N" meta="on"/>
      </Action>
      <Action id="open-mlint-message"/>
      <Action id="exit-debug">
         <Stroke code="VK_F5" shift="on"/>
         <Sequence>
            <Stroke code="VK_X" ctrl="on"/>
            <Stroke code="VK_C" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="selection-begin-line">
         <Stroke code="VK_HOME" shift="on"/>
         <Stroke alt="on" code="VK_A" ctrl="on"/>
         <Stroke code="VK_A" ctrl="on" meta="on"/>
      </Action>
      <Action id="selection-up">
         <Stroke code="VK_UP" shift="on"/>
         <Stroke alt="on" code="VK_P" ctrl="on"/>
         <Stroke code="VK_KP_UP" shift="on"/>
         <Stroke code="VK_P" ctrl="on" meta="on"/>
      </Action>
      <Action id="insert-break">
         <Stroke code="VK_ENTER"/>
         <Stroke code="VK_M" ctrl="on"/>
      </Action>
      <Action id="show-preferences">
         <Sequence>
            <Stroke code="VK_C" ctrl="on"/>
            <Stroke code="VK_S" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="comment">
         <Sequence>
            <Stroke code="VK_C" ctrl="on"/>
            <Stroke code="VK_C" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="save">
         <Sequence>
            <Stroke code="VK_X" ctrl="on"/>
            <Stroke code="VK_S" ctrl="on"/>
         </Sequence>
         <Sequence>
            <Stroke code="VK_J" ctrl="on"/>
            <Stroke code="VK_S" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="scroll-down">
         <Stroke code="VK_UP" ctrl="on"/>
         <Stroke alt="on" code="VK_P"/>
         <Stroke code="VK_P" meta="on"/>
      </Action>
      <Action id="add-cell-divider">
         <Sequence>
            <Stroke code="VK_U" ctrl="on"/>
            <Stroke code="VK_I" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="redo">
         <Stroke alt="on" code="VK_Y" shift="on"/>
         <Stroke code="VK_Y" meta="on" shift="on"/>
      </Action>
      <Action id="beep"/>
      <Action id="next-cell">
         <Sequence>
            <Stroke code="VK_U" ctrl="on"/>
            <Stroke code="VK_N" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="find-and-replace">
         <Stroke alt="on" code="VK_R"/>
         <Stroke code="VK_R" meta="on"/>
      </Action>
      <Action id="uncomment">
         <Stroke code="VK_T" ctrl="on"/>
         <Sequence>
            <Stroke code="VK_C" ctrl="on"/>
            <Stroke code="VK_X" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="prev-cell">
         <Sequence>
            <Stroke code="VK_U" ctrl="on"/>
            <Stroke code="VK_P" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="create-new-favorite">
         <Sequence>
            <Stroke code="VK_J" ctrl="on"/>
            <Stroke code="VK_S" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="selection-end-line">
         <Stroke code="VK_END" shift="on"/>
         <Stroke alt="on" code="VK_E" ctrl="on"/>
         <Stroke code="VK_E" ctrl="on" meta="on"/>
      </Action>
      <Action id="eval-cell">
         <Stroke code="VK_ENTER" ctrl="on"/>
         <Sequence>
            <Stroke code="VK_U" ctrl="on"/>
            <Stroke code="VK_R" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="selection-forward">
         <Stroke code="VK_RIGHT" shift="on"/>
         <Stroke alt="on" code="VK_F" shift="on"/>
         <Stroke code="VK_KP_RIGHT" shift="on"/>
         <Stroke code="VK_F" meta="on" shift="on"/>
      </Action>
      <Action id="enable-disable-breakpoint">
         <Sequence>
            <Stroke code="VK_J" ctrl="on"/>
            <Stroke code="VK_B" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="clear-text-at-prompt">
         <Stroke code="VK_ESCAPE"/>
      </Action>
      <Action id="selection-previous-word">
         <Stroke code="VK_LEFT" ctrl="on" shift="on"/>
         <Stroke alt="on" code="VK_B" ctrl="on"/>
         <Stroke code="VK_KP_LEFT" ctrl="on" shift="on"/>
         <Stroke code="VK_B" ctrl="on" meta="on"/>
      </Action>
      <Action id="evaluate-selection">
         <Stroke code="VK_F9"/>
         <Sequence>
            <Stroke code="VK_X" ctrl="on"/>
            <Stroke code="VK_D" ctrl="on"/>
         </Sequence>
         <Sequence>
            <Stroke code="VK_J" ctrl="on"/>
            <Stroke code="VK_D" ctrl="on"/>
         </Sequence>
      </Action>
      <Action id="selection-down">
         <Stroke code="VK_DOWN" shift="on"/>
         <Stroke alt="on" code="VK_N" ctrl="on"/>
         <Stroke code="VK_KP_DOWN" shift="on"/>
         <Stroke code="VK_N" ctrl="on" meta="on"/>
      </Action>
      <Action id="expand-fold">
         <Stroke code="VK_PERIOD" ctrl="on" shift="on"/>
         <Stroke code="VK_COMMA" ctrl="on"/>
      </Action>
   </Context>
   <Context id="HTMLRenderer">
      <Action id="jump-list-prev">
         <Stroke alt="on" code="VK_COMMA"/>
         <Stroke code="VK_COMMA" meta="on"/>
      </Action>
      <Action id="jump-list-next">
         <Stroke alt="on" code="VK_PERIOD"/>
         <Stroke code="VK_PERIOD" meta="on"/>
      </Action>
   </Context>
   <Context id="DeployTool">
      <Action id="open-selection">
         <Stroke code="VK_J" ctrl="on"/>
      </Action>
   </Context>
   <Context id="MATLABEditor">
      <Action id="unselect">
         <Stroke code="VK_G" ctrl="on"/>
      </Action>
      <Action follows="None" id="jump-list-prev">
         <Stroke alt="on" code="VK_COMMA"/>
         <Stroke code="VK_COMMA" meta="on"/>
      </Action>
      <Action follows="None" id="jump-list-next">
         <Stroke alt="on" code="VK_PERIOD"/>
         <Stroke code="VK_PERIOD" meta="on"/>
      </Action>
   </Context>
   <Context id="Explorer">
      <Action follows="None" id="jump-list-prev">
         <Stroke alt="on" code="VK_COMMA"/>
         <Stroke code="VK_COMMA" meta="on"/>
      </Action>
      <Action id="open-selection"/>
      <Action follows="None" id="jump-list-next">
         <Stroke alt="on" code="VK_PERIOD"/>
         <Stroke code="VK_PERIOD" meta="on"/>
      </Action>
   </Context>
   <Context id="CommandWindow">
      <Action follows="None" id="unselect">
         <Stroke code="VK_BACK_SLASH" ctrl="on"/>
         <Stroke code="VK_G" ctrl="on"/>
      </Action>
   </Context>
</CustomKeySet>