# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
if [ $HOSTNAME != "pc-str57" ]; then
    PATH=~/bin/:$PATH
    export PATH
fi


PATH=/glouton/struct/lian/develop/tools/svn_cvs/usr/local/texlive/2010/bin/x86_64-linux:$PATH; export PATH 
MANPATH=/glouton/struct/lian/develop/tools/svn_cvs/usr/local/texlive/2010/texmf/doc/man:$MANPATH; export MANPATH 
INFOPATH=/glouton/struct/lian/develop/tools/svn_cvs/usr/local/texlive/2010/texmf/doc/info:$INFOPATH; export INFOPATH
  
source /glouton/struct/lian/develop/tools/doc/bashcomp.sh
