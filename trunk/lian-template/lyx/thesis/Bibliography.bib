@article{sab1992,
abstract = {Due to the increasing capacities of computers, it has become possible to compute the homogenized behaviour of a random medium by the simulation of a representative volume element submitted to homogeneous stress and strain boundary conditions. The present work is devoted to the analysis of the approximations adopted in such a numerical method. The equivalence between stress and strain boundary conditions in the computation of the homogenized properties of an elastic medium is proved without any statistical or periodic assumption on the microstructure. Then, it is established with the aid of a recent ergodic theorem \{[\}Akcoglu \& Krengel, 1981] that the overall properties of a sufficiently large domain in an ergodic random medium are deterministic. Further, if this medium is suitably discretized, then, the Finite Elements computations of these overall properties are also deterministic. This rigorously justifies the fact that one can numerically compute the homogenized properties by the simulation of one single realization of the medium. Of course, such a computation will need a large computer memory size. It is shown however, that even if this memory size is not available, one can always numerically compute more or less sharp bounds which generalize those of Reuss and Voigt. The theoretical results are illustrated by a numerical example, and their generalization to nonlinear behaviours is discussed.},
address = {S P E S-JOURNAL DEPT, 120 BD ST GERMAIN, F-75006 PARIS, FRANCE},
author = {Sab, K.},
issn = {0997-7538},
journal = {European journal of mechanics. A. Solids},
number = {5},
pages = {585--607},
publisher = {Elsevier},
title = {{On the homogenization and the simulation of random materials}},
type = {Article},
url = {http://cat.inist.fr/?aModele=afficheN\&amp;cpsidt=5545667},
volume = {11},
year = {1992}
}
