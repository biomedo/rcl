keywords = {Aged,Aged, 80 and over,Cadaver,Female,Femur,Femur: anatomy \& histology,Femur: physiology,Femur: radiography,Finite Element Analysis,Humans,Male,Middle Aged,Models, Biological,Reproducibility of Results,Stress, Mechanical,Tomography, X-Ray Computed,Tomography, X-Ray Computed: statistics \& numerical},
month = oct,
number = {7},
pages = {515--22},
