number = {9},
pages = {2560--2563},
publisher = {OFFICE SPECTROSCOPY \&amp; SPECTRAL ANALYSIS},
title = {{Study on Microwave Digestion of Gypsum for the Determination of Multielement by ICP-OES and ICP-MS}},
url = {http://apps.isiknowledge.com.gate6.inist.fr/full\_record.do?product=UA\&search\_mode=GeneralSearch\&qid=1\&SID=V1IjBHafDKG1EJPnfCg\&page=1\&doc=1\&colname=WOS},
